{
  "name": "react-layer",
  "version": "1.0.0",
  "description": "A simple react layer component",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "npm run lint -- --fix",
    "dev": "better-npm-run build:dev",
    "build": "better-npm-run build:prod",
    "test": "better-npm-run test",
    "prepublish": "npm run build"
  },
  "betterScripts": {
    "build:dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "rimraf lib && webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/l644738595/react-layer.git"
  },
  "keywords": [
    "react",
    "layer",
    "react-layer",
    "react-component"
  ],
  "author": "liang jian <liangjian2017@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/l644738595/react-layer/issues"
  },
  "homepage": "https://github.com/l644738595/react-layer#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.7",
    "csswring": "^6.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jasmine-core": "^2.8.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.3.2",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.6.1",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
